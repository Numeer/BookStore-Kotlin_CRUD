            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguageTwo"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_124pxv"
                android:layout_width="wrap_content"
                android:paddingBottom="@dimen/_1pxv"
                android:paddingStart="@dimen/_19pxh"
                android:paddingTop="@dimen/_3pxv"
                android:singleLine="true"
                android:text="@{frameTwelveVM.frameTwelveModel.txtLanguageTwo}"
                style="@style/txtSolidRounded_2" tools:text="@string/lbl_update2"/>
        </LinearLayout>
    </FrameLayout>
</layout>
