<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
<variable name="mainpageVM" type="com.nomisapplication.app.modules.mainpage.data.viewmodel.MainpageVM"/>
    </data>
<LinearLayout android:gravity="start" android:id="@+id/linearColumn"
android:layout_height="match_parent"
android:layout_width="match_parent"
android:orientation="vertical" style="@style/groupStylewhite_A700">
<LinearLayout android:gravity="end"
    android:id="@+id/linearColumngroupeleven"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:orientation="vertical" style="@style/groupStylewhite_A700">
    <LinearLayout android:id="@+id/linearColumnrectangleten"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_8pxh"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylecyan_200">
        <ImageView android:gravity="start"
            android:id="@+id/imageRectangleTen"
            android:layout_height="@dimen/_10pxh"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            android:src="@drawable/img_rectangle10"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle10"/>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearRowvector"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageVector"
                android:layout_height="@dimen/_12pxh"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_width="@dimen/_18pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_gray_900"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_900"/>
            <LinearLayout android:id="@+id/linearRowvectorOne"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_234pxh"
                android:layout_weight="0.84"
                android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVectorOne"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_black_900"/>
                <FrameLayout
                    android:id="@+id/frameStackrectangleone"
                    android:layout_height="@dimen/_57pxv"
                    android:layout_marginStart="@dimen/_23pxh" android:layout_width="@dimen/_52pxh">
                    <ImageView
                        android:id="@+id/imageRectangleOne"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_52pxv"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_rectangle1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle1"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGroupTwelve"
                        android:layout_gravity="top|end"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:paddingBottom="@dimen/_2pxv"
                        android:paddingEnd="@dimen/_7pxh"
                        android:paddingStart="@dimen/_6pxh"
                        android:paddingTop="@dimen/_2pxv"
                        android:singleLine="true"
                        android:text="@{mainpageVM.mainpageModel.txtGroupTwelve}"
                        style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_2"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <EditText
        android:background="@drawable/rectangle_bg_teal_50_radius_10"
        android:drawableEnd="@drawable/img_vector_black_900_19x14"
        android:drawablePadding="@dimen/_30pxh"
        android:drawableStart="@drawable/img_search"
        android:ellipsize="end"
        android:hint="@string/lbl_search"
        android:id="@+id/etGroupEleven" android:inputType="text"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_16pxh"
        android:layout_marginStart="@dimen/_17pxh"
        android:layout_marginTop="@dimen/_25pxv"
        android:layout_width="match_parent"
        android:paddingBottom="@dimen/_13pxv"
        android:paddingEnd="@dimen/_18pxh"
        android:paddingStart="@dimen/_15pxh"
        android:paddingTop="@dimen/_12pxv"
        android:singleLine="true"
        android:text="@={mainpageVM.mainpageModel.etGroupElevenValue}"
        android:textColorHint="@color/black_900_3f"
        style="@style/etSolidRounded" tools:ignore="Autofill"/>
    <LinearLayout android:id="@+id/linearRowtrendingbook"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_17pxh"
        android:layout_marginStart="@dimen/_17pxh"
        android:layout_marginTop="@dimen/_34pxv"
        android:layout_width="match_parent" android:orientation="horizontal">

        <TextView
            android:id="@+id/txtTrendingBook"
            style="@style/txtRobotobold18"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.88"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="Books" />

        <ImageView android:id="@+id/imageVectorTwo"
            android:layout_height="@dimen/_4pxh"
            android:layout_marginBottom="@dimen/_9pxv"
            android:layout_marginStart="@dimen/_236pxh"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_width="@dimen/_16pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_vector_black_900_4x16"
            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_black_900_4x16"/>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        tools:context=".MainActivityF">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerViewBooks"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="@dimen/_16pxh"
            android:clipToPadding="false" />

    </RelativeLayout>

    <LinearLayout android:gravity="start"
        android:id="@+id/linearRowrectanglesix"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/_38pxh"
        android:layout_marginStart="@dimen/_17pxh"
        android:layout_marginTop="@dimen/_200pxv"
        android:layout_width="match_parent" android:orientation="horizontal">
        <FrameLayout android:id="@+id/frameStackrectanglesix"
            android:layout_height="@dimen/_63pxv"
            android:layout_width="@dimen/_284pxh">
            <LinearLayout
                android:id="@+id/linearColumnrectanglesix"
                android:layout_gravity="top|end"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_11pxv"
                android:layout_marginEnd="@dimen/_39pxh"
                android:layout_marginStart="@dimen/_39pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="wrap_content" android:orientation="vertical">
                <View android:background="@color/gray_101"
                    android:id="@+id/lineRectangleSix"
                    android:layout_height="@dimen/_4pxv" android:layout_width="@dimen/_85pxh"/>
                <View android:background="@color/gray_101"
                    android:id="@+id/lineRectangleSeven"
                    android:layout_height="@dimen/_4pxv"
                    android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_85pxh"/>
            </LinearLayout>

            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/btnAddABook"
                android:layout_gravity="end"
                android:layout_height="@dimen/_63pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_width="@dimen/_209pxh"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@string/lbl_add_a_book"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_add_a_book"/>
        </FrameLayout>

    </LinearLayout>
</LinearLayout>
</LinearLayout>
</layout>